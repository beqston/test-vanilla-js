{
  "version": 2,
  "routes": [
    {
      "src": "/data.json",
      "dest": "/data.json",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      "src": "/assets/(.*)",
      "dest": "/public/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}